{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "buildCommand": "mkdir -p /tmp/cache && chmod -R 777 /tmp/cache && composer install",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.php"
      }
    ]   
  }